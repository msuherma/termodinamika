    "# { PUMP }\r\n",
    "\r\n",
    "#sat liq, thus x = 0\r\n",
    "p1 = 10\r\n",
    "p2 = 2000\r\n",
    "v = 1/mp_water.ds(p=p1)[0]\r\n",
    "\r\n",
    "w_p = v* (p2-p1)\r\n",
    "print(f'Work/Kerja yang dibutuhkan oleh pompa = {round(float(w_p),1)} kJ/kg')"
   ]
